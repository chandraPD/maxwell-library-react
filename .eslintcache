L = addressUser;\n                document.getElementById('phonenumber').innerHTML = phoneNumber;\n                \n            }\n    \n    }\n    render(){\n        return(\n            <Fragment>\n            <div className=\"content-wrapper\">\n                <section className=\"content-user\">\n                    <div className=\"container-profile\">\n                        <div className=\"row\">\n                            <div className=\"col-lg-12\">\n                                <div className=\"profile-account\">\n                                    <div className=\"picture\">\n                                        <img src={avatarUser} alt=\"Admin Profile\" width='300' height='300'/>\n                                    </div>\n                                    <button \n                                    type=\"button\" \n                                    className=\"btn btn-primary edit-profile\"\n                                    name=\"editProfile\"\n                                    onClick={() => this.displayEditForm()}\n                                    >\n                                    Edit Profile\n                                    </button>\n                                </div>\n                            </div>\n                        \n                        </div>\n                        <h1 className=\"title-profile\">\n                        My Profile\n                        </h1>\n                        <div className=\"data-user\">\n                            <div className=\"row\">\n                                <div className=\"col-lg-6\">\n                                <div className=\"data\">\n                                    <h5 >Full Name</h5>\n                                    <span id=\"firstname\">Admin</span>\n                                    <hr/>\n                                </div>\n                                <div className=\"data\">\n                                    <h5 >ID Admin</h5>\n                                    <span id=\"lastname\">01</span>\n                                    <hr/>\n                                </div>\n                                <div className=\"data\">\n                                    <h5>Date of Birth</h5>\n                                    <span id=\"birthday\">19/06/2020</span>\n                                    <hr/>\n                                </div>\n                                <div className=\"data\">\n                                    <h5>Address</h5>\n                                    <span id=\"address\">Indonesia</span>\n                                    <hr/>\n                                </div>\n                                </div>\n                                <div className=\"col-lg-6\">\n                                <div className=\"data\">\n                                    <h5>Email</h5>\n                                    <span id=\"email\">example@gmail.com</span>\n                                    <hr/>\n                                </div>\n                                <div className=\"data\">\n                                    <h5 >Phone Number</h5>\n                                    <span id=\"phonenumber\">+628123456789</span>\n                                    <hr/>\n                                </div>\n                                <div className=\"data\">\n                                    {/* <a href=\"change_password.html\"><strong><u>Change Password</u></strong></a> */}\n                                    <Link to='/ChangePassword'>Change Password</Link>\n                                </div>\n                                </div>\n                            </div>\n                        </div>  \n                    </div>\n                </section>\n\n                {/* Edit Form  */}\n                <section className=\"padding-editform\">\n                <div id=\"container-editform\"